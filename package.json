    "tsoption": "^0.7.0"
  },
  "devDependencies": {
    "@solid-soda/scripts": "^1.1.3",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn soda lint-staged && yarn test && yarn types",
      "commit-msg": "yarn soda commitlint"
    }
  },
  "publishConfig": {
    "access": "public"
